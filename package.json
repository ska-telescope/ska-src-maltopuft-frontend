{
  "name": "ska-src-maltopuft-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "code-analysis": "eslint src/**/*.{ts,tsx} --ignore-path .eslintignore -f junit -o build/reports/linting.xml",
    "cypress:component": "cypress run --component --browser chrome",
    "cypress:component:ci": "cypress run --component --headless --browser chrome --reporter junit --config video=false --reporter-options mochaFile=build/reports/component-tests-[hash].xml",
    "cypress:cov": "nyc report --reporter=text --exclude-after-remap false",
    "cypress:cov:ci": "nyc report --report-dir build/reports/ --reporter=text --reporter=cobertura  --reporter=lcov",
    "cypress:e2e": "cypress run --e2e --browser chrome",
    "cypress:e2e:ci": "cypress run --e2e --headless --browser chrome --reporter junit --config video=false --reporter-options mochaFile=build/reports/e2e-tests-[hash].xml",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --config video=false",
    "cypress:run:video": "cypress run --config video=true",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "nyc:merge": "nyc merge .nyc_output .nyc_output/out.json",
    "prettier": "prettier --check \"**/*\"",
    "prettier:fix": "prettier --write \"**/*\"",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^5.16.7",
    "@mui/material": "^5.16.4",
    "@mui/x-date-pickers": "^7.11.0",
    "@tanstack/react-query": "^5.45.1",
    "axios": "^1.6.8",
    "dayjs": "^1.11.12",
    "dayjs-plugin-utc": "^0.1.2",
    "plotly.js-basic-dist": "^2.33.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-plotly.js": "^2.6.0",
    "react-router-dom": "^6.23.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.30",
    "@types/mocha": "^10.0.6",
    "@types/plotly.js": "^2.29.5",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@types/react-plotly.js": "^2.6.3",
    "@types/react-query": "^1.2.9",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "cypress": "^13.7.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "nyc": "^15.1.0",
    "prettier": "^3.2.5",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-istanbul": "^6.0.0"
  }
}
